{
  "llvm-target": "x86_64-unknown-none-elf",
  "target-pointer-width": "64",
  "data-layout": "e-m:e-p270:32:32-p271:32:32-p272:64:64-i64:64-f80:128-n8:16:32:64-S128",
  "arch": "x86_64",

  "is-builtin": false,
  "os": "none",
  "env": "",
  "abi": "",
  "vendor": "unknown",

  "cpu": "sandybridge",
  "features": "+cmov,+popcnt,-mmx,-sse,-sse2,-sse3,-ssse3,-sse4.1,-sse4.2,-3dnow,-3dnowa,-avx,-avx2,+soft-float",
  "target-endian": "little",
  "max-atomic-width": 64,
  "atomic-cas": true,

  "target-c-int-width": "32",
  "c-enum-min-bits": 8,
  "default-adjusted-cabi": "sysv64",
  "singlethread": false,
  "has-thread-local": false,
  "allow-asm": true,
  "main-needs-argc-argv": false,
  "merge-functions": "aliases",
  "panic-strategy": "abort",
  "trap-unreachable": true,
  "disable-redzone": true,
  "frame-pointer": "always",
  "supports-stack-protector": false,
  "stack-probes": { "kind": "none"},

  "executables": true,
  "no-default-libraries": true,
  "default-hidden-visibility": true,
  "position-independent-executables": true,
  "static-position-independent-executables": true,
  "relocation-model": "pic",
  "relro-level": "full",
  "code-model": "small",
  "dynamic-linking": false,
  "function-sections": false,
  "split-debuginfo": "off",
  "eh-frame-header": false,
  "requires-uwtable": false,
  "default-uwtable": false,
  "needs-plt": false,

  "linker": "rust-lld",
  "linker-flavor": "ld.lld",

  "pre-link-objects": {},
  "pre-link-objects-fallback": {},
  "post-link-objects": {},
  "post-link-args": {},
  "crt-objects-fallback": {},
  "late-link-args": {},
  "late-link-args-dynamic": {},
  "late-link-args-static": {},

  "pre-link-args": {
    "ld": [
      "--image-base=0",
      "--defsym=__KERNEL_BASE=0",
      "--entry=_start",
      "--static",
      "--no-dynamic-linker",
      "--nostdlib",
      "--no-eh-frame-hdr",
      "--pic-executable",

      "--fatal-warnings",
      "--unresolved-symbols=report-all",
      "--error-unresolved-symbols",
      "--error-limit=0",
      "--no-undefined-version",
      "--no-undefined",

      "-zcombreloc",
      "-znognustack",
      "-ztext",
      "-zcommon-page-size=2097152",
      "-zmax-page-size=2097152",

      "-zrelro",
      "-znoexecstack"
    ],
    "msvc": [
      "--orphan-handling=error",
      "--build-id=fast",
      "-zshstk",
      "-zseparate-code",

      "--strip-discarded",
      "-z", "x86-64-v2",
      "--disable-stdcall-fixup"
    ]
  }
}
