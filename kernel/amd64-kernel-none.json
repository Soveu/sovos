{
  "llvm-target": "x86_64-unknown-none",
  "is-builtin": false,
  "arch": "x86_64",
  "cpu": "sandybridge",
  "features": "+cmov,+popcnt,-mmx,-sse,-sse2,-sse3,-ssse3,-sse4.1,-sse4.2,-3dnow,-3dnowa,-avx,-avx2,+soft-float",

  "data-layout": "e-m:e-p270:32:32-p271:32:32-p272:64:64-i64:64-f80:128-n8:16:32:64-S128",
  "target-endian": "little",
  "target-pointer-width": "64",
  "target-c-int-width": "32",
  "max-atomic-width": 64,
  "atomic-cas": true,

  "env": "none",
  "os": "none",
  "executables": true,

  "panic-strategy": "abort",
  "trap-unreachable": true,
  "disable-redzone": true,
  "stack-probes": { "kind": "none"},
  "singlethread": false,

  "linker": "rust-lld",
  "linker-flavor": "ld.lld",
  "relocation-model": "pic",
  "relro-level": "full",
  "code-model": "small",
  "dynamic-linking": false,
  "function-sections": false,
  "split-debuginfo": "packed",
  "eh-frame-header": false,
  "requires-uwtable": false,
  "needs-plt": false,

  "pre-link-args": {
    "ld.lld": [
      "--image-base=0",
      "--defsym=__KERNEL_BASE=0",
      "--entry=_start",
      "--static",
      "--no-dynamic-linker",
      "--nostdlib",
      "--no-eh-frame-hdr",
      "--pic-executable",

      "--fatal-warnings",
      "--unresolved-symbols=report-all",
      "--error-unresolved-symbols",
      "--error-limit=0",
      "--no-undefined-version",
      "--no-undefined",

      "-zcombreloc",
      "-znognustack",
      "-ztext",
      "-zcommon-page-size=2097152",
      "-zmax-page-size=2097152",

      "-zrelro",
      "-znoexecstack"
    ],
    "ld": [
      "--orphan-handling=error",
      "--build-id=fast",
      "-zshstk",
      "-zseparate-code",

      "--strip-discarded",
      "-z", "x86-64-v2",
      "--disable-stdcall-fixup"
    ]
  }
}
